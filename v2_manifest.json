{
  "name": "Wikipedia IPA Reader",
  "version": "0.0.3",
  "description": "Listen to how the IPA pronounciation sounds on Wikipedia articles",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["*://en.wikipedia.org/*"],
      "js": ["content.js"],
	  "css": ["style.css"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}